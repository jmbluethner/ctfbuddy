{
  "manifest_version": 3,
  "name": "CTFBuddy",
  "version": "0.0.1",
  "description": "An easy to use Chrome extension that helps you with online CTF's and general Web OSINT",
  "icons": {
    "48": "/assets/ico_48x48.png",
    "128": "/assets/ico_128x128.png"
  },
  "action":{
    "default_popup": "index.html",
    "default_title": "CTFBuddy"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}